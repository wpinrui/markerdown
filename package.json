{
  "name": "markerdown",
  "version": "0.1.0",
  "description": "A hierarchical markdown-based knowledge management system for students",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:main\" \"yarn dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && electron .",
    "dev:renderer": "vite",
    "build": "yarn build:renderer && yarn build:main",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language-data": "^6.5.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.4",
    "@milkdown/core": "^7.17.3",
    "@milkdown/ctx": "^7.17.3",
    "@milkdown/plugin-clipboard": "^7.17.3",
    "@milkdown/plugin-history": "^7.17.3",
    "@milkdown/plugin-listener": "^7.17.3",
    "@milkdown/plugin-math": "^7.5.9",
    "@milkdown/preset-commonmark": "^7.17.3",
    "@milkdown/preset-gfm": "^7.17.3",
    "@milkdown/react": "^7.17.3",
    "@milkdown/theme-nord": "^7.17.3",
    "chokidar": "^3.5.3",
    "codemirror": "^6.0.2",
    "katex": "^0.16.27",
    "lucide-react": "^0.561.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.2.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "author": "",
  "license": "MIT"
}
